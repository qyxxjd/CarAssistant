<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <View
        style="@style/Chart.HorizontalDivider"
        android:id="@+id/item_table_top_divider"/>

    <LinearLayout style="@style/Chart.ItemLayout">

        <View style="@style/Chart.VerticalDivider"/>

        <TextView
            style="@style/Chart.ItemLable"
            android:id="@+id/item_table_lable"
            tools:text="最低油耗"/>

        <View style="@style/Chart.VerticalDivider"/>

        <TextView
            style="@style/Chart.ItemLable.Value"
            android:id="@+id/item_table_total_money"
            tools:text="78105.66元"/>

        <View style="@style/Chart.VerticalDivider"/>

        <TextView
            style="@style/Chart.ItemLable.Value"
            android:id="@+id/item_table_percentage"
            tools:text="89.23%"/>

        <View style="@style/Chart.VerticalDivider"/>
    </LinearLayout>

    <View
        style="@style/Chart.HorizontalDivider"
        android:visibility="gone"
        android:id="@+id/item_table_bottom_divider"/>
</merge>